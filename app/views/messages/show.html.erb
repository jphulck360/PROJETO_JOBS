<div class="page-header">
  <%= link_to messages_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_message_path(@message), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show message</h1>
</div>

<dl class="dl-horizontal">
  <dt>Msg:</dt>
  <dd><%= @message.msg %></dd>

  <dt>Data:</dt>
  <dd><%= @message.data %></dd>

  <dt>Hora:</dt>
  <dd><%= @message.hora %></dd>

  <dt>Status:</dt>
  <dd><%= @message.status %></dd>

</dl>
